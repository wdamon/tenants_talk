class QuestionView(generic.ListView):
    """I could have some kind of detail view as a modal on each question"""
    model = Question
    template_name = 'contest/question.html'
    context_object_name = 'output'

    def get_queryset(self):

        question_list =  Question.objects.filter(parent=self.qname)
        output = ', '.join([q.question_text for q in question_list])
